{
  "manifest_version": 2,
  "name": "CrackWare",
  "version": "5.2.2",
  "author": "TDStuart",
  "description": "by TDStuart",
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://violentegg.club/*"] ["shellshockers.io/*"] ["eggcombat.com/*"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/", "storage"],
  "icons": {
    "16" : "icon.png",
    "48" : "icon.png",
    "128": "icon.png"
  }
}